{
  "name": "app-skeleton",
  "version": "1.0.0",
  "description": "A basic skeleton for ReactJS front-end application",
  "keywords": [
    "skeleton",
    "JavaScript",
    "ES6",
    "babel",
    "webpack"
  ],
  "author": "Damien Carcel <damien.carcel@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/damien-carcel/app-skeleton.git"
  },
  "bugs": {
    "url": "https://github.com/damien-carcel/app-skeleton/issues"
  },
  "homepage": "https://github.com/damien-carcel/app-skeleton#README.md",
  "license": "MIT",
  "main": "webpack.config.js",
  "scripts": {
    "webpack:build": "webpack --mode production -p --progress",
    "lint": "node ./node_modules/tslint/bin/tslint 'src/**/*.{ts,tsx}'",
    "lint-fix": "node ./node_modules/tslint/bin/tslint 'src/**/*.{ts,tsx}' --fix",
    "webpack:serve": "webpack-dev-server --mode development --open --hot --inline",
    "webpack:serve-api": "json-server --watch db.json",
    "type-check": "tsc --noEmit --pretty"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-modal": "^3.8.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-modal": "^3.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "core-js": "3.1.4",
    "css-loader": "^3.0.0",
    "favicons-webpack-plugin": "^0.0.9",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.1.0",
    "html-webpack-template": "^6.1.0",
    "image-webpack-loader": "^5.0.0",
    "json-server": "^0.15.0",
    "less": "^3.0.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "style-loader": "^0.23.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "terser-webpack-plugin": "^1.3.0",
    "tslint": "^5.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.3",
    "webapp-webpack-plugin": "^2.7.1",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
