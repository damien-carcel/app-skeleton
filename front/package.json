{
  "name": "app-skeleton",
  "version": "1.0.0",
  "description": "A basic skeleton for ReactJS front-end application",
  "keywords": [
    "skeleton",
    "JavaScript",
    "ES6",
    "babel",
    "webpack"
  ],
  "author": "Damien Carcel <damien.carcel@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/damien-carcel/app-skeleton.git"
  },
  "bugs": {
    "url": "https://github.com/damien-carcel/app-skeleton/issues"
  },
  "homepage": "https://github.com/damien-carcel/app-skeleton#README.md",
  "license": "MIT",
  "main": "webpack.config.js",
  "scripts": {
    "build:prod": "webpack --mode production -p --progress",
    "build:dev": "webpack --mode development --progress",
    "lint": "node ./node_modules/tslint/bin/tslint 'src/**/*.{ts,tsx}'",
    "lint-fix": "node ./node_modules/tslint/bin/tslint 'src/**/*.{ts,tsx}' --fix",
    "serve": "webpack-dev-server --mode development --open --hot --inline",
    "serve-api": "json-server --watch db.json",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "@types/react-modal": "^3.2.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-modal": "^3.5.1"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.54",
    "@babel/core": "7.0.0-beta.54",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.54",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.54",
    "@babel/preset-env": "7.0.0-beta.54",
    "@babel/preset-react": "7.0.0-beta.54",
    "@babel/preset-typescript": "7.0.0-beta.54",
    "babel-loader": "8.0.0-beta.2",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.1.0",
    "html-webpack-template": "^6.1.0",
    "image-webpack-loader": "^4.3.1",
    "json-server": "^0.12.1",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "style-loader": "^0.20.3",
    "stylelint": "^9.1.3",
    "stylelint-config-standard": "^18.2.0",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "2.9.2",
    "uglify-js": "^3.3.16",
    "webapp-webpack-plugin": "^1.3.1",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  }
}
