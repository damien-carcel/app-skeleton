{
  "name": "carcel/app-skeleton",
  "description": "A basic skeleton for web API using Symfony",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "7.3.*",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "api-platform/core": "^2.1",
    "doctrine/doctrine-migrations-bundle": "^1.3|^2.0",
    "nelmio/cors-bundle": "^1.5 || ^2.0",
    "phpdocumentor/reflection-docblock": "^3.0 || ^4.0",
    "ramsey/uuid": "^3.9",
    "symfony/asset": "4.4.*",
    "symfony/console": "4.4.*",
    "symfony/dotenv": "4.4.*",
    "symfony/expression-language": "4.4.*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "4.4.*",
    "symfony/messenger": "4.4.*",
    "symfony/monolog-bundle": "^3.5",
    "symfony/security-bundle": "4.4.*",
    "symfony/twig-bundle": "4.4.*",
    "symfony/validator": "4.4.*",
    "symfony/yaml": "4.4.*",
    "webmozart/assert": "^1.5"
  },
  "require-dev": {
    "akeneo/php-coupling-detector": "^0.3.1",
    "behat/mink-browserkit-driver": "^1.3",
    "behat/mink-extension": "^2.3",
    "friends-of-behat/symfony-extension": "^2.0",
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpmd/phpmd": "^2.7",
    "phpunit/phpunit": "^8.4",
    "squizlabs/php_codesniffer": "^3.5",
    "symfony/requirements-checker": "^1.1",
    "symfony/web-server-bundle": "4.4.*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Carcel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Carcel\\Tests\\Acceptance\\": "tests/acceptance/",
      "Carcel\\Tests\\EndToEnd\\": "tests/end-to-end/",
      "Carcel\\Tests\\Fixtures\\": "tests/fixtures/",
      "Carcel\\Tests\\Integration\\": "tests/integration/",
      "Carcel\\Tests\\Unit\\": "tests/unit/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "requirements-checker": "script"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "id": "01C0Y4X3Q8GCEHX7AH8SJJB02E",
      "allow-contrib": true,
      "require": "4.4.*"
    }
  }
}
