{
  "name": "carcel/app-skeleton",
  "description": "A basic skeleton for web API using Symfony",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "7.4.*",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "api-platform/core": "^2.1",
    "doctrine/doctrine-bundle": "^2.0",
    "doctrine/doctrine-migrations-bundle": "^2.0",
    "lexik/jwt-authentication-bundle": "^2.6",
    "nelmio/cors-bundle": "^2.0",
    "phpdocumentor/reflection-docblock": "^4.0",
    "ramsey/uuid": "^3.9",
    "symfony/asset": "5.0.*",
    "symfony/console": "5.0.*",
    "symfony/dotenv": "5.0.*",
    "symfony/expression-language": "5.0.*",
    "symfony/flex": "^1.6",
    "symfony/framework-bundle": "5.0.*",
    "symfony/messenger": "5.0.*",
    "symfony/monolog-bundle": "^3.5",
    "symfony/security-bundle": "5.0.*",
    "symfony/twig-bundle": "5.0.*",
    "symfony/validator": "5.0.*",
    "symfony/yaml": "5.0.*",
    "webmozart/assert": "^1.5"
  },
  "require-dev": {
    "akeneo/php-coupling-detector": "^0.5",
    "behat/behat": "^3.5",
    "friends-of-behat/symfony-extension": "^2.1",
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpmd/phpmd": "^2.7",
    "phpmetrics/phpmetrics": "^2.5",
    "phpstan/phpstan": "^0.12.10",
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.5",
    "symfony/browser-kit": "5.0.*",
    "symfony/http-client": "5.0.*",
    "symfony/requirements-checker": "^1.1"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Carcel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Carcel\\Tests\\Acceptance\\": "tests/acceptance/",
      "Carcel\\Tests\\EndToEnd\\": "tests/end-to-end/",
      "Carcel\\Tests\\Fixtures\\": "tests/fixtures/",
      "Carcel\\Tests\\Integration\\": "tests/integration/",
      "Carcel\\Tests\\Unit\\": "tests/unit/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "requirements-checker": "script"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "id": "01C0Y4X3Q8GCEHX7AH8SJJB02E",
      "allow-contrib": true,
      "require": "5.0.*"
    }
  }
}
