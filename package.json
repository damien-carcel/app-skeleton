{
  "name": "app-skeleton",
  "version": "1.0.0",
  "description": "A basic skeleton for ES6 web applications",
  "keywords": [
    "skeleton",
    "JavaScript",
    "ES6",
    "babel",
    "webpack"
  ],
  "author": "Damien Carcel <damien.carcel@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/damien-carcel/app-skeleton.git"
  },
  "bugs": {
    "url": "https://github.com/damien-carcel/app-skeleton/issues"
  },
  "homepage": "https://github.com/damien-carcel/app-skeleton#README.md",
  "license": "MIT",
  "main": "webpack.config.js",
  "scripts": {
    "build:prod": "webpack --mode production -p --progress",
    "build:dev": "webpack --mode development --progress",
    "lint": "node ./node_modules/eslint/bin/eslint 'src/**/*.js'",
    "lint-fix": "node ./node_modules/eslint/bin/eslint 'src/**/*.js' --fix",
    "serve": "webpack-dev-server --mode development --open",
    "serve-api": "json-server --watch db.json"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-modal": "^3.1.10"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.1.0",
    "html-webpack-template": "^6.1.0",
    "json-server": "^0.12.1",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "style-loader": "^0.20.3",
    "stylelint": "^9.1.3",
    "stylelint-config-standard": "^18.2.0",
    "uglify-js": "^3.3.16",
    "webapp-webpack-plugin": "^1.3.1",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  }
}
