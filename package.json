{
  "name": "app-skeleton",
  "version": "1.0.0",
  "description": "A basic skeleton for ES6 web applications",
  "keywords": [
    "skeleton",
    "JavaScript",
    "ES6",
    "babel",
    "webpack"
  ],
  "author": "Damien Carcel <damien.carcel@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/damien-carcel/app-skeleton.git"
  },
  "bugs": {
    "url": "https://github.com/damien-carcel/app-skeleton/issues"
  },
  "homepage": "https://github.com/damien-carcel/app-skeleton#README.md",
  "license": "MIT",
  "main": "webpack.config.js",
  "scripts": {
    "build:prod": "webpack --env=prod -p --progress",
    "build:dev": "webpack --env=dev --progress",
    "lint": "node ./node_modules/eslint/bin/eslint 'src/**/*.js'",
    "lint-fix": "node ./node_modules/eslint/bin/eslint 'src/**/*.js' --fix",
    "serve": "webpack-dev-server --env=dev --open"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.4.1",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^1.1.4",
    "html-webpack-plugin": "^2.30.1",
    "html-webpack-template": "^5.6.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "style-loader": "^0.18.2",
    "stylelint": "^8.1.1",
    "stylelint-config-standard": "^17.0.0",
    "uglify-js": "^3.0.27",
    "webpack": "^3.5.4",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0"
  }
}
